                    <h1 class="page-header">SODcamp</h1>
                                                          <div class="region region-content">
    <section id="block-system-main" class="block block-system clearfix">

      
  <article id="node-136" class="node node-page clearfix" about="/page/barcamp" typeof="foaf:Document">


  <header>
            <span property="dc:title" content="SODcamp" class="rdf-meta element-hidden"></span><span property="sioc:num_replies" content="0" datatype="xsd:integer" class="rdf-meta element-hidden"></span>
    
    

